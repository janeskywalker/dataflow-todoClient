{

    "extends": "eslint:recommended",

    "rules": {

        "no-console": 1,

        "no-unused-vars": [
            "warn",
            { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }
        ]
    },

    "env": {
        "browser": true,
        "es6": true,
        "commonjs": true
    },

    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    }
    
}